{
  "parser": "babel-eslint",
  "extends": [
    "airbnb/base",
    "plugin:import/errors"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  },
  "rules": {
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "no-console": 0
  },
  "env": {
    "mocha": true,
    "jest": true
  },
  "globals": {
    "client": true,
    "LOGS_FOLDER": true,
    "INFO": true,
    "ERROR": true,
    "WARN": true
  }
}
