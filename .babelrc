{
  "passPerPreset": true,
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "babel-plugin-inline-import",
      {
        "extensions": [".gql", ".graphql"]
      },
      "@babel/plugin-transform-runtime"
    ],
    "@babel/plugin-transform-arrow-functions",
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}
